{
  "name": "rivalbot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev-server": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"npm run start\"",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "buildWindows": "npx pkg -d --targets latest-windows-x64 .",
    "buildLinux": "npx pkg -d --targets latest-linux-arm64 ."
  },
  "author": "Ladybunne",
  "license": "UNLICENSED",
  "bin": "dist/index.js",
  "pkg": {
    "scripts": ["dist/**/*.js", "dist/commands/*.js"],
    "outputPath": "builds"
  },
  "devDependencies": {
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "pkg": "^5.8.1",
    "prisma": "^4.11.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "discord.js": "^14.6.0",
    "node-schedule": "^2.1.1",
    "tesseract.js": "^4.0.2",
    "tsc-watch": "^5.0.3"
  }
}
