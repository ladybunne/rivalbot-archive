{
  "name": "rivalbot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "node ./dist/index.js",
    "dev-server": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"npm run start\"",
    "lint": "eslint . --ext .ts",
    "buildWindows": "npx pkg -debug --targets latest-windows-x64 .",
    "buildWindows2": "npx pkg -debug --targets latest-windows-x64 --outputPath build \"dist/index.js\"",
    "buildLinux": "npx pkg --targets latest-linux-arm64 .",
    "test": "jest --passWithNoTests"
  },
  "author": "Ladybunne",
  "license": "UNLICENSED",
  "bin": "dist/index.js",
  "pkg": {
    "scripts": [
      
    ],
    "assets": [
      "dist/**/*.js",
      "dist/commands/*.js",
      ".env"
    ],
    "outputPath": "build"
  },
  "devDependencies": {
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "pkg": "^5.8.1",
    "pkg-fetch": "^3.4.2",
    "prisma": "^4.11.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "discord.js": "^14.6.0",
    "node-schedule": "^2.1.1"
  }
}
